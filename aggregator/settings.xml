<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <profiles>
    <profile>
      <id>java</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <jdk.fork>false</jdk.fork>
        <jdk.version>${java.specification.version}</jdk.version>
        <jdk.home>${java.home}</jdk.home>
      </properties>
    </profile>
    <profile>
      <id>tomcat</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <tomcat.version>TOMCAT7</tomcat.version>
        <tomcat.download>false</tomcat.download>
        <tomcat.download.dir>/java/tomcat7BIS</tomcat.download.dir>
        <tomcat.home>/java/tomcat7</tomcat.home>
        <!--tomcat.home>E:\gzlb1060\java\tomcat7</tomcat.home-->
        <tempo>10</tempo>
      </properties>
    </profile>
  </profiles>
</settings>

