<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <profiles>
    <profile>
      <id>java7</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <jdk.fork>false</jdk.fork>
        <jdk.version>1.7</jdk.version>
        <jdk.home>/usr/lib/jvm/java-7-openjdk-i386</jdk.home>
        <!--jdk.home>E:\gzlb1060\java\jdk1.7.0\</jdk.home-->
      </properties>
    </profile>
    <profile>
      <id>tomcat7</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <tomcat.version>TOMCAT7</tomcat.version>
        <tomcat.download>false</tomcat.download>
        <tomcat.download.dir>/java/tomcat7BIS</tomcat.download.dir>
        <tomcat.home>/java/tomcat7</tomcat.home>
      </properties>
    </profile>
  </profiles>
</settings>
