<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <profiles>
    <profile>
      <id>java5</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <jdk.fork>true</jdk.fork>
        <jdk.version>1.5</jdk.version>
        <!--jdk.home>/usr/lib/jvm/jdk1.5.0_22</jdk.home-->
        <jdk.home>E:\gzlb1060\java\jdk1.5.0_22\</jdk.home>
      </properties>
    </profile>
    <profile>
      <id>tomcat5</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <tomcat.version>TOMCAT5</tomcat.version>
        <tomcat.download>true</tomcat.download>
        <tomcat.download.dir>/java/tomcat5BIS</tomcat.download.dir>
        <tomcat.home></tomcat.home>
      </properties>
    </profile>
  </profiles>
</settings>
